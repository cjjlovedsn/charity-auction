@import '../../node_modules/stylus-px2rem'

html-font-size = 25px


*
  box-sizing border-box


ul
  margin 0
  padding 0
  list-style none

img
  vertical-align middle
  border 0 none

html
body
  height 100%
  background-color #fff
  font-family 'pingfang'
  color #666

a
  color #666
  text-decoration none
  
.clearfix:before
.clearfix:after
  content ''
  display table
.clearfix:after
  clear: both

.pull-left
  float left
.pull-right
  float right
